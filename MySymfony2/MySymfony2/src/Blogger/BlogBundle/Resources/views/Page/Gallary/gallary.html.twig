<h3>Фотогалерея</h3>
<div>
    <div class="row">
        {% for i in 1..9 %}
            <div class="col-sm-4 col-xs-6 thumb">
                <a class="fancyimage" rel="group" href="images/gallery/00{{ i }}.jpg">
                    <img class="img-responsive" src="images/gallery/00{{ i }}.jpg"/>
                </a>
            </div>
        {% endfor %}

        {% for i in 10..18 %}
            <div class="col-sm-4 col-xs-6 thumb">
                <a class="fancyimage" rel="group" href="images/gallery/0{{ i }}.jpg">
                    <img class="img-responsive" src="images/gallery/0{{ i }}.jpg"/>
                </a>
            </div>
        {% endfor %}
    </div>
</div>
<script type="text/javascript" src="{{ asset('js/fancybox/jquery.fancybox.pack.js') }}"></script>
<script type="text/javascript">
    $(document).ready(function() {
        $("a.fancyimage").fancybox();
    });
</script>